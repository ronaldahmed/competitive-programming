import java.io;
import java.util.Scanner;

public class 11044{
	public static void Main(String[] args){
		Scanner sc = new Scanner(System.in);
		int k = Integer.parseInt(sc.nextLine());
		int n,m;
		while(k--){
			n = sc.nextInt();
			m = sc.nextInt();
			System.out.println( (n/3)*(m/3) );
		}
		System.out.close();
		sc.close();
	}
}
