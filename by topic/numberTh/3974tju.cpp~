#include <cstdio>
#include <iostream>
#include <cstring>
#define	mod	100

using namespace std;
int memo[2000];

int fast(int b, int e){
	int res=1;
	while(e){
		if(e&1)	res = (res*b)%mod;
		e>>=1;
		b = (b*b)%mod;
	}
	return res;
}

int main(){
	int n,m,t;
	memset(memo,-1,sizeof memo);
	cin>>t;
	while(t--){
		cin>>n>>m;
cout<<"----"<<n<<" "<<m<<endl;		
		if(memo[n]==-1){
			int ans = 0;
			for(int i=1;i<=n;++i)
				ans = (ans + fast(i,m))%mod;
			memo[n] = ans;
		}
		if(memo[n]<10)	printf("0");
		printf("%d\n",memo[n]);
	}
	return 0;
}
