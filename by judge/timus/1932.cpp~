#include <iostream>
#include <cstdio>
#include <string>
#include <algorithm>
#define	MAXN	(1<<17)

using namespace std;

string ids[MAXN];
int matches[16][4];


int main(){
	int n;
	scanf("%d\n",&n);
	memset(matches,0,sizeof matches);
	
	for(int i=0;i<n;++i){
		getline(cin,ids[i]);
		for(int j=0;j<4;++j)
			matches[s[j]][j]++;
	}
	return 0;
}
